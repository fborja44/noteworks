{
  "name": "denote-server",
  "version": "1.0.0",
  "bin": "./src/index.ts",
  "description": "Backend for Denote notes application.",
  "main": "index.js",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "seed": "ts-node ./src/tasks/seed.ts",
    "build": "tsc -p .",
    "pkg": "pkg ./src/index.ts"
  },
  "pkg": {
    "outputPath": "dist",
    "assets": [
      "./node_modules/**/*",
      "./src/**/*"
    ],
    "targets": [
      "node14-win-x64"
    ]
  },
  "author": "Francis Borja",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http": "0.0.1-security",
    "https": "^1.0.0",
    "mongodb": "^3.6.6",
    "nanoid": "^3.1.25",
    "ts-node": "^10.2.1"
  },
  "devDependencies": {
    "@types/mongodb": "^3.6.6",
    "@types/node": "^16.7.1",
    "pkg": "^5.8.0",
    "typescript": "^4.3.5"
  }
}
